{
  "name": "carbonara",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typeorm": "ts-node -P tsconfig.typeorm.json ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run typeorm -- migration:generate ./app/api/migrations/${npm_config_name:-Migration} -d ./app/api/db/data-source.ts",
    "migration:run": "npm run typeorm -- migration:run -d ./app/api/db/data-source.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d ./app/api/db/data-source.ts"
  },
  "dependencies": {
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.1",
    "lucide-react": "^0.536.0",
    "mysql2": "^3.14.3",
    "next": "15.2.4",
    "openai": "^5.12.2",
    "postcss": "^8.5.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "reflect-metadata": "^0.2.2",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "typeorm": "^0.3.25"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/node": "^20.19.9",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.2.4",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.3.6",
    "typescript": "^5"
  }
}
